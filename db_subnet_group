resource "aws_rds_subnet_group" "wordpress_db_subnet_group" {
  name       = "wordpress-db-subnet-group"
  subnet_ids = [
    aws_subnet.private_subnet_1.id, 
    aws_subnet.private_subnet_2.id
  ]
  tags = {
    Name = "WordPress DB Subnet Group"
  }

  depends_on = [
    aws_subnet.private_subnet_1,
    aws_subnet.private_subnet_2
  ]
}
